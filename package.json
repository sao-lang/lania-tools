{
    "name": "@lania-tools",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "init:changeset": "changeset init",
        "lint-staged": "lint-staged",
        "changeset": "changeset",
        "vp": "changeset version",
        "release": "pnpm build && pnpm release:only",
        "release:only": "nrm use npm && changeset publish --registry=https://registry.npmjs.org",
        "prepare": "husky install",
        "eslint": "eslint ./**/*.ts --fix",
        "build": "gulp --gulpfile scripts/gulpfile.cjs build",
        "clean:tools": "rimraf packages/tools/dist",
        "commit": "git add . && cz"
    },
    "dependencies": {
        "@lania-tools/json": "workspace:^",
        "@lania-tools/tools": "workspace:^"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.1",
        "@commitlint/cli": "^17.7.1",
        "@commitlint/config-conventional": "^17.7.0",
        "@rollup/plugin-alias": "^5.1.0",
        "@rollup/plugin-commonjs": "^26.0.1",
        "@rollup/plugin-json": "^6.0.1",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-typescript": "^11.1.5",
        "@rollup/pluginutils": "^5.1.4",
        "@types/minimatch": "^6.0.0",
        "@types/node": "^24.10.1",
        "@types/spark-md5": "^3.0.5",
        "@typescript-eslint/eslint-plugin": "^6.3.0",
        "@typescript-eslint/parser": "^6.3.0",
        "commitizen": "^4.3.0",
        "cz-customizable": "^7.0.0",
        "cz-git": "^1.12.0",
        "eslint": "8.43.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "estree-walker": "^3.0.3",
        "gulp": "^4.0.2",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.3",
        "magic-string": "^0.30.21",
        "minimatch": "^10.1.1",
        "prettier": "^3.3.3",
        "rimraf": "^5.0.1",
        "rollup": "^4.0.2",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-dts": "^6.1.1",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.36.0",
        "tslib": "^2.6.2",
        "typescript": "^5.9.3",
        "vitest": "^0.33.0"
    },
    "lint-staged": {
        "./**/*.ts": [
            "eslint --fix"
        ],
        "./**/*.js": [
            "eslint --fix"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-customizable"
        },
        "cz-customizable": {
            "config": ".czrc.cjs"
        }
    },
    "browserslist": [
        "last 2 version",
        "> 1%",
        "not dead"
    ]
}
