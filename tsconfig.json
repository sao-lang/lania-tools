{
    "compilerOptions": {
        "target": "es6",
        "module": "esnext",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true
    },
    "include": [
        "src/**/*",
        "**/*.js",
        "**/*.cjs",
        "**/*.mjs",
        "**/*.ts",
        "**/*.tsx",
        "./.eslintrc.cjs",
        "worker.d.ts"
    ],
    "exclude": ["node_modules", "dist"]
}
